{
  "name": "kakao-clone",
  "version": "1.0.0",
  "description": "kakao clone coding with vanila js and tailwind css",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "npm run build:tailwindcss && npm run build:server",
    "build:tailwindcss": "tailwindcss -i ./css/styles.css -o ./src/views/styles.css",
    "build:server": "babel src -d build",
    "build:assets": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Changyoon-Lee/kakao-clone.git"
  },
  "author": "Changyoon-Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Changyoon-Lee/kakao-clone/issues"
  },
  "homepage": "https://github.com/Changyoon-Lee/kakao-clone#readme",
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "install": "^0.13.0",
    "nodemon": "^2.0.20",
    "tailwindcss": "^3.2.4",
    "watch": "^0.13.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "pug": "^3.0.2"
  }
}
